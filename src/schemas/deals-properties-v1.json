            "documentation": "https://legacydocs.hubspot.com/docs/methods/deals/get_deal_property",
            "method": "GET"
        },
        "create": {
            "path": "deals/properties",
            "documentation": "https://legacydocs.hubspot.com/docs/methods/deals/create_deal_property",
