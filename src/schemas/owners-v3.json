{
    "resource": "crm",
    "class": "OwnerHubspot",
    "documentation": "https://developers.hubspot.com/docs/api/crm/owners",
    "version": 3,
    "latest": true,
    "actions": {
        "getAll": {
            "path": "owners",
            "method": "GET",
            "description": "Read a page of owners.",
            "iteratorIndex": "results",
            "afterIndex": "paging.next.after"
        },
        "get": {
            "path": "owners/{ownerId}",
            "method": "GET",
            "description": "Read an owner identified by {ownerId}."
        },
        "importAll": {
            "handler": "import_all",
            "description": "(Handler) Import All Contacts using offset and getAll"
        }
    }
}