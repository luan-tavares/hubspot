{
    "resource": "crm",
    "version": "v3",
    "class": "ContactHubspot",
    "documentation": "https://developers.hubspot.com/docs/api/crm/contacts",
    "methods": {
        "list": {
            "path": "objects/contacts",           
            "action": "GET"
        },
        "get": {
            "path": "objects/contacts/{contactId}",
            "action": "GET"
        },
        "create": {
            "path": "objects/contacts",
            "action": "POST"
        },
        "archive": {
            "path": "objects/contacts/{contactId}",
            "action": "DELETE"
        },
        "update": {
            "path": "objects/contacts/{contactId}",
            "action": "PATCH"
        },
        "delete": {
            "path": "objects/contacts/gdpr-delete",
            "action": "POST"
        },
        "search": {
            "path": "objects/contacts/search",
            "action": "POST"
        }
    }
}