{
    "resource": "automation",
    "version": "v4",
    "class": "WorkflowExtensionHubspot",
    "documentation": "https://developers.hubspot.com/docs/api/automation/custom-workflow-actions",
    "actions": {
        "completeCallback": {
            "path": "actions/callbacks/{callbackId}/complete",         
            "method": "POST",
            "description": "Completes the given action callback."
        },
        "batchCompleteCallbacks": {
            "path": "actions/callbacks/complete",           
            "method": "POST",
            "description": "Completes the given action callbacks."
        }
    }
}