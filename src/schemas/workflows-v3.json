{
    "resource": "automation",
    "class": "WorkflowHubspot",
    "version": 3,
    "actions": {
        "getAll": {
            "path": "workflows",
            "documentation": "https://legacydocs.hubspot.com/docs/methods/workflows/v3/get_workflows",
            "method": "GET"
        },
        "get": {
            "path": "workflows/{workflowId}",
            "documentation": "https://legacydocs.hubspot.com/docs/methods/workflows/v3/get_workflow",
            "method": "GET"
        }
    }
}