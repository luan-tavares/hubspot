{
    "resource": "crm-pipelines",
    "version": "v1",
    "class": "PipelineV1Hubspot",
    "actions": {
        "getAll": {
            "path": "pipelines/{object_type}",
            "documentation": "https://legacydocs.hubspot.com/docs/methods/pipelines/get_pipelines_for_object_type",
            "method": "GET",
            "description": "Get all pipelines for a specified object type",
            "iterator": "results"
        },
        "get": {
            "path": "pipelines/{object_type}/{pipeline_id}",
            "documentation": "",
            "method": "GET",
            "description": "Get an existing pipeline"
        },
        "create": {
            "path": "pipelines/{object_type}",
            "documentation": "https://legacydocs.hubspot.com/docs/methods/pipelines/create_new_pipeline",
            "method": "POST",
            "description": "Create a new pipeline"
        },
        "update": {
            "path": "pipelines/{object_type}/{pipeline_id}",
            "documentation": "https://legacydocs.hubspot.com/docs/methods/pipelines/update_pipeline",
            "method": "PUT",
            "description": "Update an existing pipeline"
        },
        "delete": {
            "path": "pipelines/{object_type}/{pipeline_id}",
            "documentation": "https://legacydocs.hubspot.com/docs/methods/pipelines/delete_pipeline",
            "method": "DELETE",
            "description": "Delete an existing pipeline"
        }
    }
}