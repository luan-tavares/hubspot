{
    "resource": "deals",
    "version": "v1",
    "class": "DealV1Hubspot",
    "actions": {
        "get": {
            "path": "deal/{dealId}",
            "documentation": "https://legacydocs.hubspot.com/docs/methods/deals/get_deal",
            "method": "GET"
        },
        "getAll": {
            "path": "deal/paged",
            "documentation": "https://legacydocs.hubspot.com/docs/methods/deals/get-all-deals",
            "method": "GET"
        },
        "getRecentlyModified": {
            "path": "deal/recent/modified",
            "documentation": "https://legacydocs.hubspot.com/docs/methods/deals/get_deals_modified",
            "method": "GET"
        },
        "getRecentlyCreated": {
            "path": "deal/recent/created",
            "documentation": "https://legacydocs.hubspot.com/docs/methods/deals/get_deals_created",
            "method": "GET"
        },
        "create": {
            "path": "deal",
            "documentation": "https://legacydocs.hubspot.com/docs/methods/deals/create_deal",
            "method": "POST"
        },
        "update": {
            "path": "deal/{dealId}",
            "documentation": "https://legacydocs.hubspot.com/docs/methods/deals/update_deal",
            "method": "PUT"
        },
        "delete": {
            "path": "deal/{dealId}",
            "documentation": "https://legacydocs.hubspot.com/docs/methods/deals/delete_deal",
            "method": "delete"
        },
        "batchUpdate": {
            "path": "batch-async/update",
            "documentation": "https://legacydocs.hubspot.com/docs/methods/deals/batch-update-deals",
            "method": "POST"
        }
    }
}