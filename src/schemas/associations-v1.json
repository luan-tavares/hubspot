{
    "resource": "crm-associations",
    "class": "AssociationHubspot",
    "version": 1,
    "actions": {
        "get": {
            "path": "associations/{objectId}/HUBSPOT_DEFINED/{definitionId}",
            "documentation": "https://legacydocs.hubspot.com/docs/methods/crm-associations/get-associations",
            "method": "GET"
        },
        "create": {
            "path": "associations",
            "documentation": "https://legacydocs.hubspot.com/docs/methods/crm-associations/associate-objects",
            "method": "PUT"
        },
        "delete": {
            "path": "associations/delete",
            "documentation": "https://legacydocs.hubspot.com/docs/methods/crm-associations/delete-association",
            "method": "DELETE"
        },
        "batchCreate": {
            "path": "associations/create-batch",
            "documentation": "https://legacydocs.hubspot.com/docs/methods/crm-associations/batch-associate-objects",
            "method": "PUT"
        },
        "batchDelete": {
            "path": "associations/delete-batch",
            "documentation": "https://legacydocs.hubspot.com/docs/methods/crm-associations/batch-delete-associations",
            "method": "PUT"
        }
    }
}